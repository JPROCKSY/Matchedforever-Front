<div class="web-profile inner-flex">
    <div class="back-button" (click)="backbutton();">
        <span class="material-symbols-outlined">arrow_back</span>
        <p>Back</p>
    </div>
    <div class="my-profile-detail-wrapper">
        <div class="left-profile-wrapper inner-flex inner-flex-small">
            <div class="myprofile-image-wrapper my-profile-detail-card">
                <a href="{{profileObj.profile_pic}}" *ngIf="profileObj.profile_pic" data-fancybox="gallery">
                    <img src="{{profileObj.profile_pic}}"  alt="">
                </a>
                <img src="assets/images/male.png" *ngIf="!profileObj.profile_pic && profileObj.gender == 'Male'" alt="">
                <img src="assets/images/female.png" *ngIf="!profileObj.profile_pic && profileObj.gender == 'Female'"
                    alt="">
            </div>
            <div class="gallery-images-grid">
                <div class="gallery-image-card-thumbs" *ngFor="let data of profileObj.images">
                    <a href="{{data.image}}" data-fancybox="gallery" data-caption="Test" target="_self"
                        *ngIf="data.image">
                     
                        <img src="{{ data.image }}" />
                    </a>
                </div>
            </div>
            <!-- <div class="">
                <a href="{{profileObj.bioData}}" class="w100block" target="_blank">
                    <button class="matched-forever-button button-style-primary w100block">
                        <span class="material-symbols-outlined">
                            contact_page
                        </span>
                        <p>
                            Bio Data
                        </p>
                    </button>
                </a>
            </div> -->

        </div>
        <div class="profile-basic-detail-wrapper inner-flex inner-flex-small ">
            <div class="my-profile-detail-card inner-flex inner-flex-medium">
                <div class="flex-header-rowjsc">
                    <div class="section-subtitle section-title-main-flex">
                        <p>
                            Profile Detail
                        </p>
                        <div class="bio_data" *ngIf="valid_userF">
                            <!-- *ngIf="plan_id" -->
                            <!-- <div> -->
                            <span class="material-symbols-outlined">
                                download
                            </span>
                            <!-- </div> -->
                            <a href="javascript:void(0);" (click)="getBiodata();" disabled="isGeneratingBio" >{{isGeneratingBio ? "Generating..." : "Biodata"}}</a>

                            <a id="fancyboxTrigger" data-fancybox="gallery" data-caption="Test" style="display: none;"></a>
                        </div>
                    </div>
                    <div class="inner-social-profilelinks" *ngIf="profileObj.basicInfo">
                        <ul
                            *ngIf="profileObj.basicInfo.facebook_link || profileObj.basicInfo.instagram_link || profileObj.basicInfo.linkedin_link">
                            <li *ngIf="profileObj.basicInfo.facebook_link">
                                <a href="{{profileObj.basicInfo.facebook_link}}" class="social-icon-profile"
                                    *ngIf="profileObj.basicInfo.facebook_link" target="_blank">
                                    <img src="assets/images/social-icons/fb.svg " alt="">
                                </a>
                            </li>
                            <li *ngIf="profileObj.basicInfo.instagram_link">
                                <a href="{{profileObj.basicInfo.instagram_link}}" class="social-icon-profile"
                                    *ngIf="profileObj.basicInfo.instagram_link" target="_blank">
                                    <img src="assets/images/social-icons/insta.svg" alt="">
                                </a>
                            </li>
                            <li *ngIf="profileObj.basicInfo.linkedin_link">
                                <a href="{{profileObj.basicInfo.linkedin_link}}" class="social-icon-profile"
                                    *ngIf="profileObj.basicInfo.linkedin_link" target="_blank">
                                    <img src="assets/images/social-icons/linkedin.svg " alt="">
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="detail-wrapper-grid-3">
                    <div class="section-text section-content-detail-flex">
                        <p class="content-title-p">
                            Full Name
                        </p>
                        <p class="text-capitalise">
                            {{profileObj.first_name+" "+profileObj.middle_name+" "+profileObj.last_name}}
                        </p>
                    </div>
                    <div class="section-text section-content-detail-flex" *ngIf="profileObj.phone && valid_userF">
                        <p class="content-title-p">
                            Contact Number
                        </p>
                        <a href="tel:{{profileObj.phone}}">
                            <p class="text-capitalise">
                                {{profileObj.phone}}
                            </p>
                        </a>
                    </div>
                    <div class="section-text section-content-detail-flex"
                        *ngIf="profileObj.alternate_number && valid_userF">
                        <p class="content-title-p">
                            Alternate Number
                        </p>
                        <a href="tel:{{profileObj.alternate_number}}">
                            <p class="text-capitalise">
                                {{profileObj.alternate_number}}
                            </p>
                        </a>
                    </div>
                    <div class="section-text section-content-detail-flex" *ngIf="profileObj.email && valid_userF">
                        <p class="content-title-p">
                            Email
                        </p>
                        <a href="mailto:{{profileObj.email}}">
                            <p>
                                {{profileObj.email}}
                            </p>
                        </a>
                    </div>
                    <div class="section-text section-content-detail-flex" *ngIf="profileObj.caste_name">
                        <p class="content-title-p">
                            Caste
                        </p>
                        <p class="text-capitalise">
                            {{profileObj.caste_name}}
                        </p>
                    </div>
                    <div class="section-text section-content-detail-flex" *ngIf="profileObj.religion_name">
                        <p class="content-title-p">
                            Religion
                        </p>
                        <p class="text-capitalise">
                            {{profileObj.religion_name}}
                        </p>
                    </div>
                    <div class="section-text section-content-detail-flex" *ngIf="profileObj.gender">
                        <p class="content-title-p">
                            Gender
                        </p>
                        <p class="text-capitalise">
                            {{profileObj.gender}}
                        </p>
                    </div>
                    <div class="section-text section-content-detail-flex"
                        *ngIf="profileObj.basicInfo && profileObj.basicInfo.relationshipStatusName">
                        <p class="content-title-p">
                            Relationship Status
                        </p>
                        <p class="text-capitalise">
                            {{profileObj.basicInfo.relationshipStatusName}}
                        </p>
                    </div>
                    <div class="section-text section-content-detail-flex"
                        *ngIf="profileObj.languages && profileObj.languages.length > 0">
                        <p class="content-title-p">
                            Languages
                        </p>
                        <div class="text-capitalise" *ngFor="let lang of profileObj.languages">
                            <p>
                                {{ lang.language }}
                            </p>
                        </div>
                    </div>

                </div>
            </div>
