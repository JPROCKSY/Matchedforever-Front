                                {{profileObj.basicInfo.residential_address+",
                                "+profileObj.basicInfo.residential_address_apt+",
                                "+profileObj.basicInfo.residential_city_name+",
                                "+profileObj.basicInfo.preffered_setlement_state_name+",
                                "+profileObj.basicInfo.residential_country_name+" -
                                "+profileObj.basicInfo.residential_zipcode}}
                            </p> -->
                            <p class="text-capitalise w75imp addess-wrapper">
                                <span *ngIf="profileObj.basicInfo.residential_address">
                                    {{ profileObj.basicInfo.residential_address }}
                                </span>
                                <span *ngIf="profileObj.basicInfo.residential_address_apt">
                                    , {{ profileObj.basicInfo.residential_address_apt }}
                                </span>
                                <span *ngIf="profileObj.basicInfo.residential_city_name">
