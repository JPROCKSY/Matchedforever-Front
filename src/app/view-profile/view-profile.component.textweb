                        <div class="section-content">
                            <p class="semibold-fonts content-title-p">
                                Residential
                            </p>
                        </div>
                    </div>
                    <div class="detail-wrapper-grid-2">
                        <div class="section-text section-content-detail-flex" *ngIf="valid_userF">
